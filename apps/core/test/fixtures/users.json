{
    "ns": "mosql",
    "collection": "users",
    "table": "tbl_user",
    "indexes": ["email"],
    "primary_keys": ["id"],
    "mappings": [
        {
            "mongo_key": "_id",
            "sql_column": "id",
            "sql_type": "text",
            "primary_key": true
        },
        {
            "mongo_key": "name",
            "sql_column": "full_name",
            "sql_type": "text"
        },
        {
            "mongo_key": "email",
            "sql_column": "email",
            "sql_type": "text"
        },
        {
            "mongo_key": "zip",
            "sql_column": "zip_code",
            "sql_type": "varchar(50)"
        },
        {
            "mongo_key": "city",
            "sql_column": "city",
            "sql_type": "varchar(5)"
        },
        {
            "mongo_key": "created_date",
            "sql_column": "created_date",
            "sql_type": "timestamp with time zone"
        }
    ]
}